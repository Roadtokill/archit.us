{
  "presets": ["./src/Build/babel-preset.js"],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-nullish-coalescing-operator",
    [
      "babel-plugin-module-resolver",
      {
        "root": ["."],
        "alias": {
          "@architus/facade": "../design/src",
          "@architus/lib": "../lib/src",
          "@app": "../app/src"
        }
      }
    ]
  ]
}
